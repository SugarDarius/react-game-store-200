        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@babel/runtime": "^7.3.1",
        "autoprefixer": "^9.4.7",
        "babel-loader": "^8.0.0-beta.1",
        "better-npm-run": "^0.1.1",
        "body-parser": "^1.18.3",
        "chalk": "^2.4.2",
        "classnames": "^2.2.6",
        "cors": "^2.8.5",
        "css-loader": "^2.1.0",
        "dotenv": "^6.2.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.9.1",
        "express": "^4.16.4",
        "file-loader": "^3.0.1",
        "foundation-sites": "^6.5.3",
        "http-proxy-middleware": "^0.19.1",
        "jest": "^24.1.0",
        "lodash": "^4.17.13",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "node-sass": "^4.11.0",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "react": "^16.8.1",
        "react-dom": "^16.8.1",
        "react-hot-loader": "^4.6.5",
        "react-redux": "^6.0.0",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.1",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "swagger-ui-dist": "^3.20.7",
        "uuid": "^3.3.2",
        "webpack": "^4.29.3",
        "webpack-cli": "^3.2.3",
        "webpack-dev-middleware": "^3.5.2",
        "webpack-hot-middleware": "^2.24.3",
        "webpack-node-externals": "^1.7.2"
    }
}
